<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andrey.inovasipembelajaran">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InovasiPembelajaran">
        <activity android:name=".PertanyaanPemantik"></activity>
        <activity android:name=".TentangActivity" />
        <activity android:name=".Materi" />
        <activity android:name=".PetaKonsep" />
        <activity android:name=".TujuanActivity" />
        <activity
            android:theme="@style/Theme.InovasiPembelajaranNoAction"
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>